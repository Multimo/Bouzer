{
  "name": "elm-monocle",
  "version": "1.3.0",
  "description": "Library providing purely functional abstractions to manipulate records",
  "scripts": {
    "compile": "elm-make ./src/**/*.elm --output monocle.js",
    "compile-and-watch": "echo 'Compiling elm-monocle ...' && npm run compile && onchange './src/**/*.elm' -v -- npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturopala/elm-monocle.git"
  },
  "keywords": [
    "monocle",
    "lens",
    "iso",
    "prism",
    "optional",
    "record"
  ],
  "author": "Artur Opala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arturopala/elm-monocle/issues"
  },
  "homepage": "https://github.com/arturopala/elm-monocle#readme",
  "devDependencies": {
    "elm": "^0.17.0",
    "elm-test":"^0.16.0",
    "onchange": "^2.0.0",
    "ws": "^1.0.1"
  }
}
